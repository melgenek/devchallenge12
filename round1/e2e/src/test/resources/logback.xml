<configuration>

    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>
                %d{dd-MM-yyyy HH:mm:ss} %-5level %logger{36} - %msg%n
            </pattern>
        </encoder>
    </appender>

    <logger name="dev.challenge.storage" level="DEBUG"/>

    <root level="TRACE">
        <appender-ref ref="STDOUT"/>
    </root>

</configuration>
